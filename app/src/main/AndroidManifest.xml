        </activity>
        <activity android:name=".TimePickerActivity" />
        <activity android:name=".AlertActivity" />
        <activity android:name=".CapitalActivity" />
        <activity android:name=".ConstraintActivity" />
        <activity android:name=".CountriesActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".DatePickerActivity" />
        <activity android:name=".DictionaryActivity" />
        <activity android:name=".FloatingButtonActivity" />
        <activity android:name=".FragmentActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>